{
  "description": "Screenshots webpages and sends them to webnative for storage",
  "manifest_version": 2,
  "name": "Lil' Patchy",
  "version": "0.0.1",
  "icons": {
    "32": "icons/fission-32.png"
  },
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": ["https://stupid-enormous-square-hero.fission.app/"],
      "js": ["proxy.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icons/fission-32.png",
    "default_popup": "pages/popup.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "activeTab"
  ]
}